{
	"name": "discocks",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"engines": {
		"node": "20",
		"pnpm": "9"
	},
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"cf-typegen": "wrangler types --env preview && move worker-configuration.d.ts src/",
		"db:studio": "drizzle-kit studio",
		"db:migrate:local": "drizzle-kit generate && drizzle-kit migrate",
		"db:migrate:dev": "drizzle-kit generate && drizzle-kit migrate -- --dev",
		"db:migrate:prod": "drizzle-kit generate && drizzle-kit migrate -- --prod"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "4.7.1",
		"@sveltejs/kit": "2.5.21",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@types/node": "20.14.14",
		"better-sqlite3": "11.1.2",
		"dotenv": "16.4.5",
		"drizzle-kit": "0.24.0",
		"prettier": "3.3.3",
		"prettier-plugin-organize-imports": "4.0.0",
		"prettier-plugin-svelte": "3.2.6",
		"prettier-plugin-tailwindcss": "0.6.6",
		"svelte": "5.0.0-next.210",
		"svelte-check": "3.8.5",
		"tslib": "2.6.3",
		"typescript": "5.5.4",
		"unocss": "0.61.9",
		"vite": "5.4.0",
		"wrangler": "3.70.0"
	},
	"dependencies": {
		"@unocss/reset": "0.61.9",
		"@yannicel/typed-api": "0.2.8",
		"drizzle-orm": "0.33.0",
		"zod": "3.23.8"
	}
}
